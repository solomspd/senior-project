  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug inference",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/cavaj/src/inference.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceRoot}/cavaj/src",
      "args": ["--model", "../model_checkpoint/checkpoint", "--llc", "../data/50k/java_src/AABBPool.java"],
    },
    {
      "name": "Debug Model",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/cavaj/src/main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceRoot}/cavaj/src",
      "args": ["--data_point_num", "100", "--ast_max_len", "80", "--checkpoint", "../model_checkpoints/checkpoint.pt"],
      "env": {
        // "CUDA_LAUNCH_BLOCKING": "1",
        // "CUBLAS_WORKSPACE_CONFIG": ":16:8"
      }
    },
  ]
}
